{
    "name": "comotic/laravel-medialibrary-generators",
    "description": "Collections of Path generators for Spatie laravel-medialibrary pacakge",
    "keywords": [
        "laravel-medialibrary",
        "path",
        "provider"
    ],
    "homepage": "https://github.com/comotic/laravel-medialibrary-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Perraut",
            "email": "nicolas@comotic.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-medialibrary": "^9.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Comotic\\LaravelMediaLibraryGenerators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Comotic\\LaravelMediaLibraryGenerators\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
